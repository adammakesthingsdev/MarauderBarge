{
  "compilerOptions": {
    //"module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2017",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module":"preserve",
    "inlineSourceMap": true,
    "incremental": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["pb_data/*"]
}